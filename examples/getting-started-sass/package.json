		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"nanoid": "^3.1.30",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"prismjs": "^1.25.0",
		"sass": "^1.43.4",
